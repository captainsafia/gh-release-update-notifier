{
  "name": "gh-release-update-notifier",
  "type": "module",
  "version": "1.0.1",
  "packageManager": "pnpm@10.25.0",
  "description": "Update notification API for CLIs published on GitHub Releases",
  "author": "Safia Abdalla <safia@safia.rocks> (https://safia.rocks)",
  "license": "MIT",
  "funding": "https://github.com/sponsors/captainsafia",
  "homepage": "https://github.com/captainsafia/gh-release-update-notifier#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/captainsafia/gh-release-update-notifier.git"
  },
  "bugs": "https://github.com/captainsafia/gh-release-update-notifier/issues",
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "update-notifier",
    "github-releases",
    "cli",
